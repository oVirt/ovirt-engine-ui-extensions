{
  "name": "ovirt-engine-ui-extensions",
  "version": "1.0.5",
  "description": "UI plugin that provides various extensions to oVirt administration UI, such as dashboard, migrate vm, and cluster upgrade",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://gerrit.ovirt.org/ovirt-engine-ui-extensions"
  },
  "zanataLocales": "de,es,fr,it,ja,ko,pt-BR,zh-CN,cs",
  "scripts": {
    "prebuild": "yarn test",
    "build": "NODE_ENV=production webpack -p --bail",
    "dev": "webpack -d --progress",
    "dev:watch": "yarn dev --watch",
    "intl:extract": "node zanata/extract-translations.js && rip json2pot 'extra/messages/**/*.json' -o 'extra/messages/messages.pot'",
    "intl:push-all": "cd zanata && zanata-cli push --push-type source --locales $npm_package_zanataLocales",
    "intl:pull-all": "cd zanata && zanata-cli pull --pull-type trans --locales $npm_package_zanataLocales",
    "intl:apply": "rip po2json 'extra/messages/po/messages.*.po' -m 'extra/messages/**/*.json' -o 'src/intl/translations.json'",
    "intl:normalize": "node zanata/normalize-translations.js",
    "lint": "eslint src --ext js,jsx",
    "pretest": "yarn lint",
    "test": "NODE_ENV=test karma start --single-run --no-auto-watch",
    "test:watch": "NODE_ENV=test karma start --no-single-run --auto-watch"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "=3.3.7",
    "c3": "=0.4.22",
    "classnames": "^2.2.3",
    "css-element-queries": "^0.4.0",
    "d3": "^3.5.16",
    "intl": "^1.2.4",
    "intl-messageformat": "^1.3.0",
    "jquery": "=3.4.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "patternfly": "^3.51.1",
    "patternfly-react": "^2.36.2",
    "prop-types": "^15.6.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "fs-extra": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "intl-messageformat-parser": "^1.2.0",
    "json-loader": "^0.5.7",
    "json-stable-stringify": "^1.0.1",
    "karma": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "^2.0.13",
    "mocha": "^4.1.0",
    "phantomjs-prebuilt": "^2.1.16",
    "react-intl-po": "^1.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  }
}
